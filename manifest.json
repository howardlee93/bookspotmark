{
  "name": "MarkPlace",
  "description": "Marking where you were last reading",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": [ "<all_urls>" ],
  "action": {
    "default_popup": "popup.html"
  }

}